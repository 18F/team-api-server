{
  "name": "team-api-server",
  "version": "0.2.0",
  "description": "Webhook handling server that listens for changes on .about.yml files and pushes updates to a destination repo.",
  "scripts": {
    "start": "node index.js",
    "test": "tap test/*.js",
    "lint": "eslint **/*.js; exit 0;",
    "dev-start": "nodemon index.js",
    "dev-test": "nodemon -x 'tap test/*.js'",
    "coverage": "tap --coverage-report=text-lcov test/*.js > lcov.info"
  },
  "engines": {
    "node": "^5.10.0"
  },
  "devDependencies": {
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "mockery": "^1.7.0",
    "nodemon": "^1.9.2",
    "sinon": "^1.17.4",
    "tap": "^5.7.2",
    "tape": "^4.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/18F/team-api-server.git"
  },
  "author": "James Seppi <james.seppi@gsa.gov>",
  "contributors": [
    "Atul Varma <atul.varma@gsa.gov>",
    "Mike Bland"
  ],
  "bugs": {
    "url": "https://github.com/18F/team-api-server/issues"
  },
  "homepage": "https://team-api.18f.gov/public/api/",
  "license": "CC0-1.0",
  "dependencies": {
    "cfenv": "^1.0.3",
    "dotenv": "^2.0.0",
    "githooked": "^1.1.1",
    "js-yaml": "^3.6.1",
    "request-promise": "^3.0.0",
    "winston": "^2.2.0"
  }
}
